{
  "name": "dash_alternative_viz",
  "version": "0.0.1",
  "description": "Various Python datavis bindings for Dash.",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublish": "npm run validate-init",
    "build:js-dev": "webpack --mode development",
    "build:js": "webpack --mode production",
    "build:py": "dash-generate-components ./src/lib/components dash_alternative_viz",
    "build:py-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py)",
    "build:all": "npm run build:js && npm run build:js-dev && npm run build:py",
    "build:all-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:all)"
  },
  "author": "Nicolas Kruchten nicolas@plot.ly",
  "license": "MIT",
  "dependencies": {
    "bokehjs": "^1.2.0",
    "highcharts": "^8.1.1",
    "highcharts-react-official": "^2.2.2",
    "ramda": "^0.25.0",
    "vega": "^5.4.0",
    "vega-embed": "^4.2.0",
    "vega-lite": "^3.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copyfiles": "^2.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "npm": "^6.14.6",
    "react": ">=0.14",
    "react-docgen": "^2.20.1",
    "react-dom": ">=0.14",
    "style-loader": "^0.21.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-serve": "^1.0.2"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
